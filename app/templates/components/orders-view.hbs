<div class="col-lg-8">
	<div class="panel panel-primary">
		<div class="panel-heading">
			<h3 class="panel-title"><strong>{{type}}</strong></h3>
		</div>
		<div class="panel-body">
			<table class="table">
				<thead>
					<tr>
						<td>
							Market
						</td>
						<td>
							Period
						</td>
						<td>
							Working
						</td>
						<td>
							Level
						</td>
						<td>
							Latest
						</td>
						<td>
							Good Till
						</td>
						<td></td>
					</tr>
				</thead>
				<tbody>
					{{#each model as |item|}}
						<tr>
							<td>
								{{item.marketData.instrumentName}}
							</td>
							<td>
								{{item.marketData.expiry}}
							</td>
							<td>
								{{item.workingOrderData.direction}} {{item.workingOrderData.size}}
							</td>
							<td>
								{{item.workingOrderData.level}}
							</td>
							<td>
								{{item.liveData.latest}}
							</td>
							<td>
								{{item.workingOrderData.goodTill}}
							</td>
							<td class="btn btn-primary" {{action 'close' item}}>
								Delete
							</td>
						</tr>
						{{else}}
							<td colspan="8" class="alert alert-warning col-sm-8 text-center">
								You have no {{type}}. Head to
								{{#link-to 'account.search' tagName="a" }}Search and Deal{{/link-to}} and make some!
							</td>
					{{/each}}
				</tbody>
			</table>
		</div>
	</div>
</div>
