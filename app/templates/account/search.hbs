{{search-box search="search"}}
{{#if model}}
<table class="table table-hover" id="search_results_list">
  <thead>
    <tr>
      <th>Status</th>
      <th>Market</th>
      <th>Expiry</th>
      <th>Bid</th>
      <th>Offer</th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |result|}}
      {{result-row result=result}}
    {{/each}}
  </tbody>
</table>
{{else}}
<p class="text-center">
  No markets here mate.
</p>
{{/if}}
