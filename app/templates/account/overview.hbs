<div class="panel panel-primary">
	<div class="panel-heading">
		<h3 class="panel-title">Account Details</h3>
	</div>
	<div class="panel-body">
		<div class="row">
			{{#each model as |account|}}
				<div class="col-md-6">
					{{#account-item item=account}}
						{{#link-to 'account.overview' class='btn btn-info btn-xs' }}Funds{{/link-to}}
					{{/account-item}}
				</div>
			{{/each}}
		</div>
	</div>
</div>
